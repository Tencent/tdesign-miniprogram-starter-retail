  border-width: 2px 2px 0 0;
  position: absolute;
  top: 50%;
  right: 26rpx;
  margin-top: -3px;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}
.arrows-inline::after {
  position: relative;
  left: auto;
  top: auto;
  right: auto;
  bottom: auto;
  margin-top: -4px;
  margin-left: 5px;
}
